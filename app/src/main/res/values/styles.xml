<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    <style name="CustomToolbarTheme" parent="Theme.AppCompat.NoActionBar">
        <item name="colorPrimaryDark">@color/green_300</item>
        <item name="colorAccent">@color/red_600</item>
        <item name="colorPrimary">@color/purple_500</item>
        <item name="android:textColorPrimary">@color/orange_300</item>
        <item name="android:windowBackground">@color/yellow_300</item>
        <item name="android:navigationBarColor">@color/purple_500</item>
    </style>

    !-- Base application theme. -->
    <style name="AppTheme2" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryDark">@color/purple_600</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>






</resources>
